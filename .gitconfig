[alias]
	aa = add -A .
	fa = fetch --all
	st = status
	diffc = diff --cached
	ci = commit
	co = checkout
	cp = cherry-pick
	put = push origin HEAD
	fixup = !sh -c 'git commit -m \"fixup! $(git log -1 --format='\\''%s'\\'' $@)\"' -
	squash = !sh -c 'git commit -m \"squash! $(git log -1 --format='\\''%s'\\'' $@)\"' -
	doff = reset head^
	ri = rebase --interactive
	rc = rebase --continue
	br = branch
	pruneremote = remote prune origin
	tree = log --graph --oneline --decorate --color --all
	tr = log --graph --oneline --decorate --color
	unpushed = !PROJ_BRANCH=$(git symbolic-ref HEAD | sed 's|refs/heads/||') && git log origin/$PROJ_BRANCH..HEAD
	unpulled = !PROJ_BRANCH=$(git symbolic-ref HEAD | sed 's|refs/heads/||') && git fetch && git log HEAD..origin/$PROJ_BRANCH
	add-untracked = !git status --porcelain | awk '/\\?\\?/{ print $2 }' | xargs git add
	ln = log --pretty=format:'%Cblue%h %Cred* %C(yellow)%s'
	reset-authors = commit --amend --reset-author -CHEAD
	rmbranch = "!f(){ git branch -d ${1} && git push origin --delete ${1}; };f"
	snapshot = !git stash savesnapshot: $(date)&& git stash applystash@{0}
	prune = fetch --all --prune
	pushf = push --force-with-lease
[branch]
	autosetuprebase = always
[color]
	ui = auto
[core]
	excludesfile = ~/.cvsignore
	editor = \"C:/Program Files (x86)/vim/vim74/gvim\"
	whitespace = warn
[interactive]
	singlekey = true
[merge]
	summary = true
	tool = kdiff3
[merge "bundlelock"]
	name = lock Gemfile
	driver = bundle install
[merge "bundle"]
	name = bundle
	driver = bundle
[merge "railsschema"]
	name = newer Rails schema version
	driver = "ruby -e '\n\t\tsystem %(git), %(merge-file), %(--marker-size=%L), %(%A), %(%O), %(%B)\n\t\tb = File.read(%(%A))\n\t\tb.sub!(/^<+ .*\\nActiveRecord::Schema\\.define.:version => (\\d+). do\\n=+\\nActiveRecord::Schema\\.define.:version => (\\d+). do\\n>+ .*/) do\n\t\t  %(ActiveRecord::Schema.define(:version => #{[$1, $2].max}) do)\n\t\tend\n\t\tFile.open(%(%A), %(w)) {|f| f.write(b)}\n\t\texit 1 if b.include?(%(<)*%L)'"
[push]
	default = tracking
[rebase]
	autosquash = true
[diff]
	algorithm = patience
	guitool = kdiff3
[web]
	browser = chromium-browser
[include]
	path = ~/.gitconfig.local
[credential]
	helper = manager
[difftool "kdiff3"]
	path = C:/Program Files/KDiff3/kdiff3.exe
	cmd = \"C:/Program Files/KDiff3/kdiff3.exe\" \"$LOCAL\" \"$REMOTE\"
